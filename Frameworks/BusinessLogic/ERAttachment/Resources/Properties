##  Properies Info
er.extensions.load.Properties.framework.ERAttachment=load
er.extensions.ERAttachment.hasLocalization=true

#### Attachment Setup Sample
##er.attachment.productAttachment.storageType=file
##er.attachment.productAttachment.proxyAsAttachment=false
##er.attachment.productAttachment.width=100
##er.attachment.productAttachment.height=100
##er.attachment.productAttachment.file.proxy=true
##er.attachment.productAttachment.file.filesystemPath=/Volumes/Data/Users/xxx/Desktop/products/attachments/${hash}/${pk}${ext}
##er.attachment.productAttachment.file.webPath=/products/attachments/${hash}/${pk}${ext}


#### Base Folder for File Storage Type : see ERFileAttachment
#er.attachment.file.filebasePath=

#### Migration
ERAttachment.MigrationClassPrefix=er.attachment.migrations.ERAttachment

#### Supported MIME Typelist
er.attachment.mimeTypes=video/x-ms-wmv,video/x-ms-wmp,video/x-ms-wma,video/x-ms-wm,video/msvideo,video/mpeg,video/mp4,video/x-ms-asx,video/x-ms-asf,video/3gpp2,video/3gpp,text/rtf,text/directory,image/vnd.microsoft.icon,image/svg+xml,font/truetype,font/opentype,audio/basic,audio/aiff,application/x-sit,application/x-Shockwave-Flash,application/vnd.rn-realmedia,application/vnd.rn-realaudio,application/binhex,text/javascript,application/java-archive,application/jnlp,application/macbinary,audio/mpeg,audio/mp4,audio/x-wav,image/bmp,image/eps,image/gif,image/jp2,image/jpeg,application/pdf,image/pict,image/png,image/psd,image/raw,image/tiff,image/x-pict,text/css,text/plain,text/html,text/xml,text/x-diff,application/zip,application/x-tar,application/x-gzip,application/x-octet-stream,video/quicktime,application/guitar-pro3,application/guitar-pro4,application/guitar-pro5

#### Viewer
er.attachment.mimeType.video/quicktime.viewer=er.attachment.components.viewers.ERAttachmentQuickTimeViewer
er.attachment.mimeType.image/*.viewer=er.attachment.components.viewers.ERAttachmentImageViewer
er.attachment.mimeType.default.viewer=er.attachment.components.viewers.ERAttachmentDefaultViewer
er.attachment.mimeType.application/pdf.viewer=er.attachment.components.viewers.ERAttachmentDefaultViewer
er.attachment.mimeType.unavailable.viewer=er.attachment.components.viewers.ERAttachmentUnavailableViewer

